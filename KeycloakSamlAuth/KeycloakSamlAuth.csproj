<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="certificates\instructions to generate certificate.txt" />
    <None Remove="certificates\private-key-site-visits-dev.key" />
    <None Remove="certificates\site-visits-dev.crt" />
    <None Remove="certificates\site-visits-dev.p12" />
    <None Remove="certificates\site-visits-dev.pfx" />
    <None Remove="certificates\sslconfig.cnf" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="ITfoxtec.Identity.Saml2" Version="4.15.3" />
    <PackageReference Include="ITfoxtec.Identity.Saml2.MvcCore" Version="4.15.3" />
  </ItemGroup>

</Project>
